{
    "manifest_version": 3,
    "name": "Be Quotify by Hamza ATAOUI",
    "description": "Recevez une citation motivante chaque jour.",
    "version": "1.0",
    "permissions": [
      "storage"
    ],

    "host_permissions": [
    "https://zenquotes.io/*"
  ],
  
    "action": {
      "default_popup": "popup.html"
    },

    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  